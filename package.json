{
  "private": true,
  "scripts": {
    "dev-vite": "vite",
    "prod": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
    "server:dev": "php artisan serve",
    "dev": "concurrently \"npm run development\" \"npm run server:dev\" \"npm run browser-sync\"",
    "browser-sync": "browser-sync start --proxy http://127.0.0.1:8000 --files 'resources/**/*,public/**/*'",
    "watch": "npm run development -- --watch",
    "db:migrate": "php artisan migrate",
    "db:drop": "php artisan migrate:reset",
    "db:seed": "php artisan db:seed",
    "composer:install": "composer install",
    "setup": "npm run db:migrate && npm run db:seed && npm run storage:link",
    "storage:link": "php artisan storage:link"
  },
  "devDependencies": {
    "axios": "^1.1.2",
    "cross-env": "^7.0.3",
    "laravel-vite-plugin": "^0.7.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.62.0",
    "sass-loader": "^12.1.0",
    "vite": "^4.0.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^5.1.1",
    "bootstrap": "^5.2.3",
    "bootstrap-vue": "^2.23.1",
    "concurrently": "^8.0.1",
    "file-saver": "^2.0.5",
    "install": "^0.13.0",
    "json2csv": "^6.0.0-alpha.2",
    "katex": "^0.16.6",
    "laravel-mix": "^6.0.49",
    "mathlive": "^0.92.1",
    "npm": "^9.6.5",
    "pdfmake": "^0.2.7",
    "vue": "^3.2.47",
    "vue-good-table-next": "^0.2.1",
    "vue-i18n": "^9.2.2",
    "vue-json-csv": "^2.1.0",
    "vue-loader": "^17.0.1",
    "vue-multiselect": "^3.0.0-beta.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-spinners": "^1.2.2"
  }
}
